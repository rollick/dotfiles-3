#
# Set user-specific environment variables here for Openbox
#
if [[ -d "${XDG_CONFIG_HOME:-${HOME}/.config}/openbox/scripts" ]]; then
	export PATH="${PATH}:${XDG_CONFIG_HOME:-${HOME}/.config}/openbox/scripts"
fi

export GNOME_DESKTOP_SESSION_ID=1

# Fix GTK2+
if [[ -f "${HOME}/.gtkrc-2.0" ]]; then
	export GTK2_RC_FILES="${HOME}/.gtkrc-2.0"
fi

# Remove missing accessibility bus messages
export NO_AT_BRIDGE=1
# vim: ft=sh
