# Global aliases
alias -g C="|wc -l"
alias -g ES="2>&1"
alias -g G="|grep"
alias -g GE="|grep -E"
alias -g GI="|grep -i"
alias -g GV="|grep -v"
alias -g H="|head"
alias -g K="|keep"
alias -g NE="2>|/dev/null"
alias -g NO="&>|/dev/null"
alias -g P='|${PAGER}'
alias -g S="|sort"
alias -g SU="|sort -u"
alias -g U="|uniq"
alias -g T="|tail"
alias -g X="|xargs"
alias -g X0="|xargs -0"
